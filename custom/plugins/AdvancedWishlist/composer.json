{
    "name": "advanced-wishlist/advanced-wishlist",
    "description": "Enterprise Advanced Wishlist System for Shopware 6 - PHP 8.4 Edition",
    "type": "shopware-platform-plugin",
    "version": "2.0.0",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "shopware/core": "~6.7.0",
        "symfony/framework-bundle": "~7.2",
        "symfony/validator": "~7.2",
        "symfony/rate-limiter": "~7.2",
        "symfony/cache": "~7.2",
        "doctrine/orm": "^3.0",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.12",
        "symfony/phpunit-bridge": "~7.2"
    },
    "extra": {
        "shopware-plugin-class": "AdvancedWishlist\\AdvancedWishlist",
        "label": {
            "de-DE": "Erweitertes Wunschlisten-System",
            "en-GB": "Advanced Wishlist System"
        }
    },
    "autoload": {
        "psr-4": {
            "AdvancedWishlist\\": "src/"
        },
        "files": [
            "../../vendor/shopware/core/Framework/DataAbstractionLayer/Field/CustomFieldsField.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AdvancedWishlist\\Tests\\": "tests/"
        }
    }
}
